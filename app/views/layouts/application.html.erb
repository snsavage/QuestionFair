<!DOCTYPE html>
<html>

<%= render 'layouts/header' %>

<body>
  
  <div class="container">

    <div class="row">
      <%= render 'layouts/nav' %>
    </div>

    <div class="row">
      <div class="col-md-12">
        <ol class="breadcrumb">
          <%= render_breadcrumbs :tag => :li, :separator => "" %>
        </ol>
      </div>
    <div>
    
    <div class="row">
      <%= render 'layouts/flash' %>
    </div>
    
    <div class="row">
      <%= yield %>
    </div>

    <div class="row">
      <%= render 'layouts/footer' %>
    </div>

  </div>
</body>

<%= debug(params) if Rails.env.development? %>

</html>

